<p id="notice"><%= notice %></p>

<h1>Gbp Usds</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Spot exchange rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gbp_usds.each do |gbp_usd| %>
      <tr>
        <td><%= gbp_usd.date %></td>
        <td><%= gbp_usd.spot_exchange_rate %></td>
        <td><%= link_to 'Show', gbp_usd %></td>
        <td><%= link_to 'Edit', edit_gbp_usd_path(gbp_usd) %></td>
        <td><%= link_to 'Destroy', gbp_usd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gbp Usd', new_gbp_usd_path %>
